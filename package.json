{
  "name": "bloq-it-sw-challenge",
  "version": "1.0.0",
  "description": "Bloq.it SW Challenge",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc",
    "start": "tsc && node --unhandled-rejections=strict ./dist/server.js",
    "debug": "export DEBUG=* && npm run start",
    "dev": "NODE_ENV=development tsx watch src/server --unhandled-rejections=strict",
    "dev-debug": "export DEBUG=* && npm run dev",
    "prepare": "husky install",
    "test": "jest",
    "test-debug": "export DEBUG=* && jest"
  },
  "author": "Pedro Algarvio",
  "license": "Apache-2.0",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "express-winston": "^4.2.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-config-eslint": "^11.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
